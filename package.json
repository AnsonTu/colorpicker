{
    "name": "colorpicker",
    "productName": "colorpicker",
    "version": "3.0.0",
    "description": "Colorpicker is a desktop tool with Electron to get and save colors code quickly for OSX, Windows and Linux!",
    "main": "dist/main.js",
    "repository": "git@github.com:Toinane/new-colorpicker.git",
    "keywords": [
        "Colorpicker",
        "Colors",
        "Picker",
        "Eyedropper",
        "Shades",
        "Tints",
        "RGB",
        "HSL"
    ],
    "os": [
        "win32",
        "darwin",
        "linux"
    ],
    "engines": {
        "node": ">=12"
    },
    "author": {
        "name": "Toinane",
        "url": "https://toinane.crea-th.at"
    },
    "bugs": "https://github.com/toinane/colorpicker/issues",
    "homepage": "https://colorpicker.fr",
    "license": "CECILL-2.1",
    "scripts": {
        "dev:electron": "webpack --config ./scripts/webpack.electron.config.js --mode development && electron ./dist/main.js",
        "dev:react": "webpack-dev-server --config ./scripts/webpack.react.config.js --mode development",
        "build": "rm -rf ./dist && webpack --config ./scripts/webpack.electron.config.js --mode production",
        "release": "node scripts/build-release.js",
        "nightly": "node scripts/build-nightly.js"
    },
    "dependencies": {
        "i18next": "^19.5.4",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "@types/react": "^16.9.41",
        "@types/react-dom": "^16.9.8",
        "@typescript-eslint/eslint-plugin": "^3.5.0",
        "@typescript-eslint/parser": "^3.5.0",
        "css-loader": "^3.6.0",
        "electron": "^9.0.5",
        "electron-builder": "^22.7.0",
        "eslint": "^7.4.0",
        "eslint-plugin-react": "^7.20.3",
        "html-webpack-plugin": "^4.3.0",
        "prettier": "^2.0.5",
        "source-map-loader": "^1.0.1",
        "style-loader": "^1.2.1",
        "svg-url-loader": "^6.0.0",
        "ts-loader": "^7.0.5",
        "typescript": "^3.9.6",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    }
}
